e.getters.getAdditionals;\n    });\n\n    const desc = computed(() => {\n      console.log(infos.value[0]);\n      return shortenDesc(infos.value[0][\"Sadrzaj članka\"], 150);\n    });\n\n    onMounted(async function () {\n      store.dispatch(\"Reset\");\n      await store.dispatch(\"setCategory\", {\n        category: \"kultura\",\n      });\n    });\n\n    return {\n      infos,\n      desc,\n    };\n  },\n};\n</script>"],"sourceRoot":""}]}